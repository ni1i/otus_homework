[server]
pxeserver ansible_host=127.0.0.1 ansible_port=2222 ansible_user=vagrant ansible_private_key_file=.vagrant/machines/pxeserver/virtualbox/private_key

[client]
pxeclient ansible_host=127.0.0.1 ansible_port=2223 ansible_user=vagrant ansible_private_key_file=.vagrant/machines/pxeclient/virtualbox/private_key